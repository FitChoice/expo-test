{
	"name": "fitchoice",
	"version": "1.0.0",
	"main": "expo-router/entry",
	"scripts": {
		"start": "expo start",
		"android": "expo run:android",
		"ios": "expo run:ios",
		"web": "expo start --web",
		"deploy": "npx expo export -p web && npx eas-cli@latest deploy",
		"type-check": "tsc --noEmit",
		"lint": "eslint src --ext .ts,.tsx --max-warnings 0",
		"lint:fix": "eslint src --ext .ts,.tsx --fix",
		"format": "prettier --write \"src/**/*.{ts,tsx,js,jsx}\" \"*.{md,json}\"",
		"format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\" \"*.{md,json}\"",
		"check": "pnpm format && pnpm lint && pnpm type-check"
	},
	"dependencies": {
		"@thinksys/react-native-mediapipe": "^0.0.16",
		"expo": "^54.0.1",
		"expo-constants": "~18.0.8",
		"expo-linking": "~8.0.7",
		"expo-router": "~6.0.0",
		"expo-splash-screen": "~31.0.8",
		"expo-status-bar": "~3.0.7",
		"nativewind": "^4.0.1",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-native": "0.81.4",
		"react-native-reanimated": "~4.1.0",
		"react-native-safe-area-context": "~5.6.0",
		"react-native-screens": "~4.16.0",
		"react-native-web": "^0.21.0",
		"tailwindcss": "^3.4.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.37.0",
		"@types/react": "~19.1.10",
		"eslint": "^9.37.0",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-security": "^3.0.1",
		"globals": "^16.4.0",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"typescript": "~5.9.2",
		"typescript-eslint": "^8.46.0"
	},
	"private": true,
	"packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
